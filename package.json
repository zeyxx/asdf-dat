{
  "name": "asdf-burn-engine",
  "version": "2.0.0",
  "description": "ASDF Burn Engine - Optimistic Burn Protocol",
  "main": "dist/cli.js",
  "bin": {
    "asdf-burn-engine": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "build:all": "tsc && anchor build",
    "clean": "rm -rf dist target",
    "dev": "ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "test": "anchor test",
    "test:unit": "mocha",
    "test:all": "npm run test:unit && anchor test",
    "compile": "tsc",
    "daemon": "ts-node src/cli.ts",
    "init": "ts-node scripts/init-dat-state.ts",
    "init-token": "ts-node scripts/init-token-stats.ts",
    "create-token": "ts-node scripts/create-token-v2.ts",
    "set-root": "ts-node scripts/set-root-token.ts",
    "check-fees": "ts-node scripts/check-fees.ts",
    "check-state": "ts-node scripts/check-dat-state.ts",
    "volume": "ts-node scripts/generate-volume.ts",
    "sell": "ts-node scripts/sell-spl-tokens-simple.ts",
    "cycle": "ts-node scripts/execute-ecosystem-cycle.ts",
    "validate": "ts-node scripts/validate-before-mainnet-launch.ts",
    "transfer-admin": "ts-node scripts/transfer-admin.ts",
    "accept-admin": "ts-node scripts/accept-admin.ts"
  },
  "dependencies": {
    "@coral-xyz/anchor": "0.31.1",
    "@pump-fun/pump-sdk": "^1.22.1",
    "@pump-fun/pump-swap-sdk": "^1.7.7",
    "@solana/spl-token": "^0.4.0",
    "@solana/web3.js": "^1.91.0",
    "axios": "^1.13.2",
    "dotenv": "^16.4.1",
    "express": "^5.1.0",
    "mime-types": "^3.0.2",
    "nft.storage": "^7.2.0",
    "socket.io": "^4.8.1",
    "ws": "^8.18.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bn.js": "^5.2.0",
    "@types/chai": "^5.2.3",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/mime-types": "^3.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.11.5",
    "@types/ws": "^8.18.1",
    "chai": "^6.2.1",
    "mocha": "^11.7.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
